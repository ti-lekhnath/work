<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_picking_type_inherit_form" model="ir.ui.view">
            <field name="name">stock.picking.type.form</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock_delivery.view_picking_type_form_delivery"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='print_label']" position="after">
                    <field name="export_to_csv" widget="toggle"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
